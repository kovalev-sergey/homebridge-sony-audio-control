{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "script"
    },
    "rules": {
        "semi": "warn",
        "no-unused-vars": "warn",
        "keyword-spacing": "error",
        "no-prototype-builtins": "off"
    }
}
